earning = int(input("Выручка: "))
cost = int(input("Издержки: "))
if earning < cost:
    print("Убыток")
elif earning == cost:
    print("Кол-во выручки = кол-ву издержек")
elif earning > cost:
    print("Прибыль")
    profit = round((earning - cost) / earning, 2)
    print(f"Рентабельность: {profit}")
    workers = int(input("Введите количество сотрудников: "))
    profit_per = round((earning - cost) / workers, 2)
    print(f"Прибыль на одного человека: {profit_per}")
